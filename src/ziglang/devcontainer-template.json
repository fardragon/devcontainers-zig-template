{
    "id": "ziglang",
    "version": "0.2.0",
    "name": "Devcontainers Ziglang template",
    "description": "Devcontainers template for Ziglang. Contains ZLS and Zig extensions for VSCode",
    "documentationURL": "https://github.com/fardragon/devcontainers-zig-template/tree/main/src/zig",
    "licenseURL": "https://github.com/fardragon/devcontainers-zig-template/blob/main/LICENSE",
    "options": {
        "zigVersion": {
            "type": "string",
            "description": "Zig compiler version",
            "proposals": [
                "0.13.0",
                "0.14.0",
                "0.14.1"
            ],
            "default": "0.14.1"
        }
    },
    "platforms": [
        "Any"
    ]
}

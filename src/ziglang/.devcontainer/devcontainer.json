{
	"name": "Devcontainers Zig",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"ZIG_VERSION": "0.13.0",
			"MINISIGN_VERSION": "0.11"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-azuretools.vscode-docker",
				"ziglang.vscode-zig",
				"ianic.zig-language-extras",
				"lorenzopirro.zig-snippets"
			]
		}
	}
}